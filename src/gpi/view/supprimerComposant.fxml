<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import java.lang.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="118.0" prefWidth="400.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="gpi.view.SupprimerComposant">
  <children>
    <HBox id="HBox" alignment="CENTER" minWidth="-1.0" spacing="5.0" AnchorPane.bottomAnchor="10.0" AnchorPane.rightAnchor="10.0">
      <children>
        <Button mnemonicParsing="false" onAction="#handleOk" prefWidth="80.0" text="Supprimer" />
        <Button mnemonicParsing="false" onAction="#handleCancel" prefWidth="80.0" text="Annuler" />
      </children>
    </HBox>
      <GridPane layoutX="7.0" layoutY="8.0" prefHeight="173.0" prefWidth="307.0" AnchorPane.bottomAnchor="50.0" AnchorPane.leftAnchor="5.0" AnchorPane.rightAnchor="5.0" AnchorPane.topAnchor="5.0">
        <columnConstraints>
          <ColumnConstraints halignment="LEFT" hgrow="SOMETIMES" maxWidth="215.0" minWidth="10.0" prefWidth="183.0" />
          <ColumnConstraints halignment="CENTER" hgrow="SOMETIMES" maxWidth="232.0" minWidth="10.0" prefWidth="207.0" />
        </columnConstraints>
        <rowConstraints>
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
        </rowConstraints>
         <children>
            <Label text="Nom du composant à supprimer" />
            <ComboBox fx:id="comboboxnom" onAction="#handlechange" prefWidth="200.0" GridPane.columnIndex="1" />
            <Label prefHeight="17.0" prefWidth="225.0" text="Caractéristiques du composant" GridPane.rowIndex="1" />
            <ComboBox fx:id="comboboxcarac" prefWidth="200.0" GridPane.columnIndex="1" GridPane.rowIndex="1" />
         </children>
      </GridPane>
  </children>
</AnchorPane>
